<?xml version="1.0" encoding="UTF-8"?><widget xmlns = "http://www.w3.org/ns/widgets"	xmlns:gap = "http://phonegap.com/ns/1.0"	id        = "com.pasto.memotest"	versionCode="1"	version   = "1.0">	<name>Memotest</name>	<description>Memotest</description>	<author href="" email="">Georgex</author>	<preference name="phonegap-version" value="2.9.0" />	<preference name="orientation" value="landscape" />	<preference name="prerendered-icon" value="false" />	<preference name="android-minSdkVersion" value="7" />	<preference name="android-installLocation" value="auto" />	<preference name="splash-screen-duration" value="2000" />    <preference name="detect-data-types" value="true" />    <preference name="fullscreen" value="true" />    <plugin name="Media" value="org.apache.cordova.AudioHandler" />    <plugin name="Media" value="CDVSound" />	<icon src="images/mobile/memo1.png" gap:platform="android" gap:density="ldpi" />	<icon src="images/mobile/memo1.png" gap:platform="android" gap:density="mdpi" />	<icon src="images/mobile/memo2.png" gap:platform="android" gap:density="hdpi" />	<icon src="images/mobile/memo2.png" gap:platform="android" gap:density="xhdpi" />	    <gap:splash src="images/mobile/splash.jpg" gap:platform="android" gap:density="ldpi" />    <gap:splash src="images/mobile/splash.jpg" gap:platform="android" gap:density="mdpi" />	<gap:splash src="images/mobile/splash.jpg" gap:platform="android" gap:density="hdpi" />	<gap:splash src="images/mobile/splash.jpg" gap:platform="android" gap:density="xhdpi" /></widget>